{
    "version": 2,
    "builds": [
        {
            "src": "api/search.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/shopping.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/fetch.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/search",
            "dest": "/api/search.py"
        },
        {
            "src": "/api/shopping",
            "dest": "/api/shopping.py"
        },
        {
            "src": "/api/fetch",
            "dest": "/api/fetch.py"
        }
    ]
}